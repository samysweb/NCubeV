[
	[
		-400 <= x1 & x1 <= 400 &
		-400 <= x2 & x2 <= 400 &
		10 < c & c <= 80 &
		1 < w & w <= 30 & 
    	-400 <= g1 & g1 <= 400 &
    	-400 <= g2 & g2 <= 400 &
		-1000 <= ystr & ystr <= 1000 &
		-1000 <= y1dir & y1dir <= 1000
	] 
]
-> [
	-1.0001 <= ystr & ystr <= 1.0001 &
	-1.0001 <= y1dir & y1dir <= 1.0001
]